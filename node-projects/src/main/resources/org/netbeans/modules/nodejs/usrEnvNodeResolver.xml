<!DOCTYPE MIME-resolver PUBLIC
          "-//NetBeans//DTD MIME Resolver 1.0//EN" 
          "http://www.netbeans.org/dtds/mime-resolver-1_0.dtd">
<MIME-resolver>
    <file>
        <fattr name="known-ant-project-file" text="false"/>
        <exit/>
    </file>
    <file>
        <ext name="java"/>
        <exit/>
    </file>
    <file>
        <ext name="js"/>
        <exit/>
    </file>
    <file>
        <ext name="xml"/>
        <exit/>
    </file>
    <file>
        <ext name="txt"/>
        <exit/>
    </file>
    <file>
        <ext name="json"/>
        <exit/>
    </file>
    <file>
        <ext name="gif"/>
        <exit/>
    </file>
    <file>
        <ext name="jpg"/>
        <exit/>
    </file>
    <file>
        <ext name="png"/>
        <exit/>
    </file>
    <file>
        <ext name="properties"/>
        <exit/>
    </file>
    <file>
        <!--<pattern value="/usr/bin/env node" range="24"/>-->        
        <!--<magic hex="2123752f7273622f6e69652f766e6e20646f0a65"/>-->
        <!--<magic hex="23212f7573722f62696e2f656e75206e6f6465"/>-->
        <magic hex="23212f7573722f62696e2f656e76206e6f6465"/>
        <resolver mime="text/javascript"/>
    </file>
</MIME-resolver>
